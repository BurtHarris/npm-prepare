    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "program": "${workspaceRoot}/bin/npm-prepare",
            "env": {
                "NODE_DEBUG": ""
            }
        },
        {
            "type": "node",
            "request": "launch",
            "name": "prepublish",
            "program": "${workspaceRoot}/bin/npm-prepare",
            "env": {
                "npm_lifecycle_event":"prepublish",
                "npm_lifecycle_script":"npm-prepare"
            }
        },
        {
            "type": "node",
            "request": "launch",
            "name": "test",
            "program": "${workspaceRoot}/test.js",

        }
    ]
}